<%= simple_form_for [@Client,@service], :html => { :class => 'form-horizontal' } do |f| %>
   <%= f.association :route, label_method: :description%>
     <%= link_to "Ver rutas disponibles", routes_path, :class => "btn btn-lg boton" %>
   <%= f.association :payment_type, label_method: :description%>

  <%= f.association :driver, label_method: :name_driver%>
   <%= f.button :submit ,:class => 'btn-primary' %>
  <%= link_to t('.Cancel', :default => t("Retornar")),
                paginaprincipal_usuario_index_path, :class => 'btn btn-default' %>

<% end %>

