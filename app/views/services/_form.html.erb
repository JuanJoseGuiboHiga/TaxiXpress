<%= simple_form_for(@service) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :address_origin %>
    <%= f.input :address_end %>
    <%= f.input :price %>
    <%= f.input :paymentype %>
    <%= f.input :observation %>
    <%= f.association :client %>
    <%= f.association :driver %>
    <%= f.association :valuation %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
